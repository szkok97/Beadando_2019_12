{
    "editor.minimap.enabled": false,
    "eslint.alwaysShowStatus": true,
    "eslint.enable": true,
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "vue",
            "autoFix": true
        }
    ],
    "vetur.validation.template": false,
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.formatOnSave": false,
    },
    "[javascriptreact]": {
        "editor.formatOnSave": false,
    },
    "[typescript]": {
        "editor.formatOnSave": false,
    },
    "[typescriptreact]": {
        "editor.formatOnSave": false,
    },
    "[vue]": {
        "editor.formatOnSave": false,
    },
    "spellright.language": [
        "en",
        "hu"
    ],
    "spellright.documentTypes": [
        "latex"
    ],
    "terminal.integrated.rendererType": "dom",
    "files.autoSave": "afterDelay",
    "files.exclude": {
        "**/.git": true,
        "**/.gitignore": false,
        "**/.vscode": false,
        "**/tsconfig.json": false,
        "**/tslint.json": false,
        "**/package-lock.json": true,
        "**/dist": true,
        "**/public": false,
        "**/node_modules": true,
        "**/*.js.map": true,
        "**/*.npmrc": true
    },
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetchPeriod": 60,
    "git.autofetch": true
}
